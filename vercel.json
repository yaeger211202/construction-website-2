{
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    { "src": "/api/notify", "dest": "/api/notify.js" }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(css|js|svg)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}

